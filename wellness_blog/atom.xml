---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Sydney Wong's Wellness Blog</title>
	<link href="http://sydneymwong.github.io/wellness_blog/atom.xml" rel="self"/>
	<link href="http://sydneymwong.github.io/wellness_blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://sydneymwong.github.io/wellness_blog</id>
	<author>
		<name>Sydney Wong</name>
		<email>sydneymwong@gmail.com</email>
	</author>

	{% for post in site.posts %}
		{% if post.categories contains 'wellness' %}
			<entry>
				<title>{{ post.title }}</title>
				<link href="http://sydneymwong.github.io{{ post.url }}"/>
				<updated>{{ post.date | date_to_xmlschema }}</updated>
				<id>http://sydneymwong.github.io{{ post.id }}</id>
				<content type="html">{{ post.content | xml_escape }}</content>
			</entry>
		{% endif %}
	{% endfor %}

</feed>
